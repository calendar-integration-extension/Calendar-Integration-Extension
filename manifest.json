{
    "manifest_version": 3,
    "name": "Google Calendar Extension",
    "permissions": ["activeTab", "identity", "notifications", "alarms", "storage"],
    "description": "A Google Calendar Integrated Extension with a Pomodoro timer to help keep users productive throughout the work day.",
    "version": "1.0.0",
    "icons": {
        "16":"/images/2-16.png",
        "48":"/images/2-48.png",
        "128":"/images/2-128.png"
